{
  "name": "coffee-budget-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "cypress:open": "start-server-and-test dev http://localhost:3000 'cypress open'",
    "cypress:run": "start-server-and-test dev http://localhost:3000 'cypress run'",
    "cypress:debug": "cypress open --config video=false",
    "test:e2e": "npm run cypress:run",
    "test:all": "npm run test:ci && npm run test:e2e"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
