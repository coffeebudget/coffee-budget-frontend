# Production Environment Variables Template for Railway Frontend
# Copy this file to .env.production and fill in the actual values

# Auth0 Production Configuration
AUTH0_CLIENT_ID=[YOUR_AUTH0_CLIENT_ID]
AUTH0_CLIENT_SECRET=[YOUR_AUTH0_CLIENT_SECRET]

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend.railway.app
NEXT_PUBLIC_AUTH0_AUDIENCE=https://your-api-identifier
NEXT_PUBLIC_AUTH0_ISSUER=https://YOUR_AUTH0_TENANT.auth0.com

# NextAuth Configuration
NEXTAUTH_SECRET=[YOUR_NEXTAUTH_SECRET]
NEXTAUTH_URL=https://your-frontend.railway.app
